@model Microsoft.AspNet.Identity.EntityFramework.IdentityRole

@{
    ViewBag.Title = Resources.Roles_Edit_cshtml_Execute_Edit;
}

<h2>Edit Role</h2>

@Html.ActionLink(Resources.Roles_Edit_cshtml_Execute_List_Roles, "Index") | @Html.ActionLink(Resources.Roles_Edit_cshtml_Execute_Manage_User_Role, "ManageUserRoles")
<hr />
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    @Html.HiddenFor(model => model.Id)
    <div>
        Role name
    </div>
    <p>
        @Html.TextBoxFor(model => model.Name)
    </p>
    <input type="submit" value="Save" />
}