@model IEnumerable<GoAber.Models.DataRemovalAudit>

@{
    ViewBag.Title = Resources.AuditRm_Title;
}

<h2>Resources.AuditRm_Title</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.User.Nickname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.message)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.dataRemoved)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.date)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.User.Nickname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.message)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.dataRemoved)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.date)
        </td>
        <td>
            @Html.ActionLink(Resources.AuditRm_Details, "Details", new { id=item.Id }) |
        </td>
    </tr>
}

</table>
