        <td>
            @Html.DisplayFor(modelItem => item.challenge.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.user.email)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idUserChallenge }) |
